{
  "name": "client_v1",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@editorjs/checklist": "^1.1.0",
    "@editorjs/code": "^2.4.1",
    "@editorjs/delimiter": "^1.1.0",
    "@editorjs/editorjs": "^2.13.0",
    "@editorjs/embed": "^2.2.0",
    "@editorjs/header": "^2.2.3",
    "@editorjs/image": "^2.3.0",
    "@editorjs/inline-code": "^1.3.0",
    "@editorjs/link": "^2.1.2",
    "@editorjs/list": "^1.3.4",
    "@editorjs/marker": "^1.2.0",
    "@editorjs/paragraph": "^2.5.0",
    "@editorjs/quote": "^2.2.0",
    "@editorjs/raw": "^2.1.1",
    "@editorjs/simple-image": "^1.3.1",
    "@editorjs/table": "^1.2.0",
    "@rematch/core": "^1.1.0",
    "@rematch/loading": "^0.5.1",
    "@rematch/persist": "^1.1.6",
    "@rematch/select": "^2.0.4",
    "add": "^2.0.6",
    "apollo-boost": "^0.4.2",
    "aws-appsync": "^1.8.0",
    "aws-appsync-react": "^1.2.8",
    "axios": "^0.18.0",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "re-reselect": "^3.1.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "slugify": "^1.3.4",
    "styled-components": "^4.2.0",
    "uuid": "^3.3.2",
    "yarn": "^1.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snapshot",
    "Serve": "serve -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "init": "yarn install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-snap": "^1.23.0",
    "react-snapshot": "^1.3.0"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "/other",
      "/content-management",
      "/404"
    ],
    "exclude": [
      "/signup",
      "/other-path/exclude-me/**"
    ],
    "snapshotDelay": 300
  },
  "reactSnap": {
    "port": 45678,
    "source": "build",
    "destination": null,
    "concurrency": 4,
    "include": [
      "/",
      "/other"
    ],
    "userAgent": "ReactSnap",
    "headless": true,
    "puppeteer": {
      "cache": true
    },
    "puppeteerArgs": [],
    "puppeteerIgnoreHTTPSErrors": false,
    "publicPath": "/",
    "minifyCss": {},
    "minifyHtml": {
      "collapseBooleanAttributes": true,
      "collapseWhitespace": true,
      "decodeEntities": true,
      "keepClosingSlash": true,
      "sortAttributes": true,
      "sortClassName": false
    },
    "viewport": {
      "width": 480,
      "height": 850
    },
    "sourceMaps": true,
    "fixWebpackChunksIssue": "CRA1",
    "removeBlobs": true,
    "fixInsertRule": true,
    "skipThirdPartyRequests": false,
    "cacheAjaxRequests": false,
    "http2PushManifest": false,
    "ignoreForPreload": [
      "service-worker.js"
    ],
    "preconnectThirdParty": false,
    "inlineCss": true,
    "saveAs": "html",
    "crawl": true,
    "waitFor": false,
    "externalServer": false,
    "removeStyleTags": false,
    "preloadImages": false,
    "asyncScriptTags": false,
    "removeScriptTags": false
  }
}
